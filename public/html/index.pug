doctype html
html(lang='en', ng-app="app")
	head
		title Insert title here
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		link(rel='stylesheet', href='../css/master.css')

	body
		#log
			nav#leftNav
				h4 Log
				a(ui-sref="home")
					i.fa.fa-plus-square#goToAdd
				a(ui-sref="log")
					i.fa.fa-bars#goToLog
			#entryField
				input(type="text", name="name", value="", placeholder="Went to the gym this morning with Genevieve.  Exercised on the elliptical for 45 minutes.")
				#markerAssign
					i.fa.fa-user
						p Person
					i.fa.fa-map-marker
						p Place
					i.fa.fa-lemon-o
						p Action

			nav#rightNav
				h4 Get
				i.fa.fa-search
				i.fa.fa-bar-chart
		ui-view

		// Font Awesome
		script(src="https://use.fontawesome.com/416e68c692.js")
		// Angular Cdn
		script(type="text/javascript", src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js")
		script(type="text/javascript", src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.min.js")
		// jquery Cdn
		script(src="http://code.jquery.com/jquery-3.1.0.min.js")
		// Angular files
		script(type="text/javascript", src="../js/angular/app.js")
		script(type="text/javascript", src="../js/angular/ctrl.js")
		script(type="text/javascript", src="../js/angular/srvc.js")
		// Personal etc js
		script(type="text/javascript", src="../js/script.js")
